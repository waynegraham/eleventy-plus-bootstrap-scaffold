<meta property="og.site_name" content="{{ site.title }}">
<meta property="og:title" content="{{ title or site.title}}">
<meta property="og:description" content="{{ description or site.description}}">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ page.url | url | absoluteUrl(site.url) }}">
<meta property="og:image" content="https://v1.opengraph.11ty.dev/{{ site.url }}">
<meta property="og:image:alt" content="{{ site.og_image_alt }}">
<meta property="og:locale" content="{{ site.local or meta.locale }}">


{# Favicon #}
<link rel="icon" href="{{ '/assets/favicons/favicon.ico' | url }}">

